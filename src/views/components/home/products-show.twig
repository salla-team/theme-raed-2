{% for products_show in component.products_show %}
    <div class="text-black w-full">
        
       
   
 
        <h3 class="" data-swiper-parallax="-300">
            {{ products_show }}
        </h3>

    </div>
{% endfor %}



{% for product in component.product %}
    <div class="text-black w-full">
        
       
       <img 
    data-swiper-parallax="-700" 
    src="{{ product.image }}" 
    alt="{{   product.image}}" 
    class="w-full h-auto"
/>
 <p data-swiper-parallax="-700">
            {{ product.name}}
        </p>
        <h3 class="" data-swiper-parallax="-300">
            {{ product.price}}
        </h3>

    </div>
{% endfor %}